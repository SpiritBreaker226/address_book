<%= form_for @contact do |f| %>
	<%= f.label :first_name %>
	<%= f.text_field :first_name %>
	<br><br>
	<%= f.label :last_name %>
	<%= f.text_field :last_name %>
	<br><br>
	<%= f.label :address %>
	<%= f.text_field :address %>
	<br><br>
	<%= f.label :picture_url %>
	<%= f.url_field :picture_url %>
	<br><br>
	<%= f.label :phone %>
	<%= f.phone_field :phone %>
	<br><br>
	<%= f.label :email %>
	<%= f.email_field :email %>
	<br><br>
	<%= f.label :age %>
	<%= f.number_field :age %>
	<br><br>
	<%= f.label :birthday %>
	<%= f.date_field :birthday %>
	<br><br>
	<%= f.submit %>
<% end %>